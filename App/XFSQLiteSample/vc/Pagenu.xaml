<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XFSQLiteSample.vc.Pagenu">
    <ContentPage.Content>
        <StackLayout  BackgroundColor="#b7ffff" Margin="20,35,20,20">
            <Entry x:Name="nameEntry" TextColor="black" Placeholder="Digite seu nome de utilizador" />
            <Entry x:Name="password" TextColor="black" Placeholder="Digite sua palavra-passe" />
            <Button TextColor="black" Background="White" Text="Registar"
                Clicked="OnButtonClicked" />
           
        </StackLayout>
    </ContentPage.Content>
</ContentPage>